root ::= SubjectAndObject
SubjectAndObject ::= "{"   ws   "\"subject\":"   ws   string   ","   ws   "\"subject_type\":"   ws   string   ","   ws   "\"object\":"   ws   string   ","   ws   "\"object_type\":"   ws   string   "}"
SubjectAndObjectlist ::= "[]" | "["   ws   SubjectAndObject   (","   ws   SubjectAndObject)*   "]"
string ::= "\""   ([^"]*)   "\""
boolean ::= "true" | "false"
ws ::= [ \t\n]*
number ::= [0-9]+   "."?   [0-9]*
stringlist ::= "["   ws   "]" | "["   ws   string   (","   ws   string)*   ws   "]"
numberlist ::= "["   ws   "]" | "["   ws   string   (","   ws   number)*   ws   "]"
